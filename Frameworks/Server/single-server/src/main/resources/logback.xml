<configuration>
    <!-- reference: https://github.com/slick/slick/blob/master/common-test-resources/logback.xml -->
    <logger name="slick" level="OFF" />
    <logger name="com.zaxxer.hikari" level="OFF" />
    <!--<logger name="slick.jdbc.JdbcBackend.statement" level="DEBUG" />-->
    <!--<logger name="slick.jdbc.StatementInvoker.result" level="DEBUG" />-->
    <!--<logger name="slick.jdbc.JdbcBackend.benchmark" level="DEBUG" />-->

    <logger name="cool.graph" level="DEBUG" />

    <root level="INFO">
        <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
            <encoder class="net.logstash.logback.encoder.LogstashEncoder" />
        </appender>
    </root>
</configuration>
